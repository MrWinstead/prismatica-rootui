    "browserify": "^16.2.0",
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "file-system": "^2.2.2",
    "fs": "0.0.1-security",
    "google-protobuf": "^3.5.0",
    "gridfs-stream": "^1.1.1",
    "grpc": "^1.10.1",
    "grpc-tools": "^1.6.6",
