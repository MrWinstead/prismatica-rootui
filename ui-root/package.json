    "npm": "^6.0.0",
    "protocol-buffers": "^4.0.4",
    "radium": "^0.24.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-dropzone": "^4.2.9",
    "react-markdown": "^3.3.0",
    "react-mounter": "^1.2.0",
    "react-router": "^4.2.0",
